	<div class="form-actions">
    	<%=f.submit "Create Page", :class=>"btn btn-primary form-btn","data-loading-text"=>"Saving..."%> or <%=link_to "cancel",spud_admin_pages_path,:class => "btn"%>
    </div>

<%end%>


  
